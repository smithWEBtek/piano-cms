<h4>Edit your information below:</h4>
<form action="/students/<%="#{@student.id}/edit"%>" method="POST">
  <input id="hidden" type="hidden" name="_method" value="patch"> 
  <input type="text" name="student[username]" id="student_<%=@student.id%>" value="<%= @student.username %>"> 
  <input type="text" name="student[password]" id="student_<%=@student.password%>" value="<%= @student.password %>">
  <input type="submit" type="submit"/>
</form>