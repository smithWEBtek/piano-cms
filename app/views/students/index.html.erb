<h4>All Students and their SongLists:</h4>

<% @all_students.order(:username).each do |student| %>
  <h4><a href="/students/<%="#{student.id}"%>"><%= student.username.upcase %>:  </a></h4>
<% student.songs.each do |song|%>
  <a href="/songs/<%="#{song.id}"%>"><%= song.name %></a>
<% end %>
<% end %>